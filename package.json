{
  "name": "phaser-plugin-advanced-timing",
  "version": "0.5.0",
  "description": "Shows FPS, frame intervals, draw count, and other performance info",
  "main": "index.js",
  "scripts": {
    "prestart": "open index.html",
    "start": "coffeebar --watch * example/*",
    "preversion": "eslint .",
    "pub": "np --no-cleanup",
    "update": "cp -v node_modules/phaser/build/phaser.js node_modules/dat.gui/build/dat.gui.js{,.map} example/vendor/"
  },
  "author": "samme",
  "license": "MIT",
  "directories": {
    "example": "example"
  },
  "keywords": [
    "phaser",
    "phaser-plugin"
  ],
  "dependencies": {
    "phaser": "^2.6.2"
  },
  "devDependencies": {
    "coffeebar": "^1.0.0",
    "dat.gui": "github:dataarts/dat.gui"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/samme/phaser-plugin-advanced-timing.git"
  },
  "bugs": {
    "url": "https://github.com/samme/phaser-plugin-advanced-timing/issues"
  },
  "homepage": "https://github.com/samme/phaser-plugin-advanced-timing#readme"
}
